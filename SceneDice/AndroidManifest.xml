<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.wanderphone.superdice"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".Start"
        	      android:screenOrientation="portrait"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

		<activity android:name="Defined"
		android:screenOrientation="portrait">
		</activity>
		<activity android:name="SceneDice"
		android:screenOrientation="portrait">
		</activity>
    	<activity android:name="Defined3"
    	android:screenOrientation="portrait"></activity>
   		<activity android:name="Defined4"
   		android:screenOrientation="portrait"></activity>
    	<activity android:name="Defined5"
    	android:screenOrientation="portrait"></activity>
    	<activity android:name="Defined6"
    	android:screenOrientation="portrait"></activity>
    	<activity android:name="Setting"
    	android:screenOrientation="portrait"></activity>
    	<activity android:name="HelpActivity"
		android:screenOrientation="portrait">
		</activity>
		<activity android:name="AboutActivity"
		android:screenOrientation="portrait">
		</activity>
		
		<!-- AdView SDK for AdMob -->
		<activity android:name="com.google.ads.AdActivity" 
        android:configChanges="orientation|keyboard|keyboardHidden"/>
    
    	<!-- AdView SDK for millennialmedia -->
    	<activity android:name="com.millennialmedia.android.MMAdViewOverlayActivity" />
    	<activity android:name="com.millennialmedia.android.VideoPlayer"  />
    	
    	<!-- AdView SDK for Wooboo -->
    	<meta-data android:value="1" android:name="Market_ID"/>
    	
    	<!-- AdView SDK for Youmi --> 
    	<activity android:name="net.youmi.android.AdActivity"
				  android:configChanges="keyboard|keyboardHidden|orientation"/>
		<!-- AdView SDK for Vpon -->
		<activity android:name="com.vpon.adon.android.WebInApp" android:screenOrientation="portrait"
        		  android:configChanges="orientation|keyboardHidden|navigation|keyboard"/>
        <!-- AdView SDK for AirAd -->       
		<activity android:name="com.mt.airad.MultiAD"
				  android:theme="@android:style/Theme.Translucent.NoTitleBar"
				  android:configChanges="orientation|keyboard|keyboardHidden"/>
	<activity android:name="MMAdViewOverlayActivity" android:screenOrientation="portrait"/>
    </application>
    
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
    <!-- AdView permission add -->
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.READ_PHONE_STATE" />
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-sdk android:minSdkVersion="3" />

</manifest> 